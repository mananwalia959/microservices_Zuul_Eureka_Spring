package io.manan.micro.models;

public class Rating {
	private String userId;
	private int rating;
	public String getUserId() {
		return userId;
	}
	public void setUserId(String userId) {
		this.userId = userId;
	}
	public int getRating() {
		return rating;
	}
	public void setRating(int rating) {
		this.rating = rating;
	}
	public Rating(String userId, int rating) {
		this.userId = userId;
		this.rating = rating;
	}
	public Rating() {
	}
	
	
}
